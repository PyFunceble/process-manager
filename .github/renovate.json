{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "ignorePaths": [],
  "separateMajorMinor": true,
  "assignees": [
    "funilrys"
  ],
  "baseBranchPatterns": [
    "dev"
  ],
  "packageRules": [
    {
      "matchDatasources": [
        "pypi"
      ],
      "matchManagers": [
        "pip_requirements"
      ],
      "groupName": "pip deps"
    },
    {
      "matchManagers": [
        "github-actions"
      ],
      "groupName": "github-actions deps"
    }
  ]
}
